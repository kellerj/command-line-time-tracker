{
  "name": "command-line-time-tracker",
  "version": "2.0.0-0",
  "description": "Time Tracker",
  "main": "./src/commands/tt.js",
  "scripts": {
    "lint": "eslint src --ext .js",
    "test": "nyc mocha --require babel-register --reporter spec test/**/*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:mocha": "mocha --require babel-register --reporter spec test/**/*.test.js",
    "test:watch": "mocha --require babel-register --reporter spec test/**/*.test.js --watch",
    "mocha": "mocha --require babel-register",
    "eslint": "eslint",
    "nyc": "nyc",
    "preversion": "npm run lint && npm test",
    "version": "git add src",
    "postversion": "git push && git push --tags",
    "tt": "DEBUG=*,-babel node -r babel-register src/commands/tt.js"
  },
  "author": {
    "name": "Jonathan Keller",
    "email": "keller.jonathan@gmail.com"
  },
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellerj/command-line-time-tracker.git"
  },
  "bin": {
    "tt": "./src/commands/tt.js"
  },
  "dependencies": {
    "chalk": "2.1.x",
    "commander": "git+https://github.com/tj/commander.js.git#3367806abf354cb420477ffe1a9dcaeb7388392f",
    "date-fns": "^1.29.0",
    "debug": "3.0.x",
    "easy-table": "1.1.x",
    "inquirer": "3.2.x",
    "inquirer-autocomplete-prompt": "0.11.x",
    "moment": "2.18.x",
    "mongodb": "^2.2.31",
    "rx": "4.1.x",
    "sprintf-js": "1.1.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2"
  },
  "nyc": {
    "exclude": [
      "src/commands/**",
      "coverage/**",
      "test/**"
    ],
    "check-coverage": false,
    "lines": 90,
    "statements": 90,
    "functions": 100,
    "branches": 75,
    "reporter": [
      "lcov",
      "text-summary",
      "text",
      "html"
    ],
    "watermarks": {
      "lines": [
        75,
        90
      ],
      "functions": [
        90,
        100
      ],
      "branches": [
        50,
        75
      ],
      "statements": [
        75,
        90
      ]
    },
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
