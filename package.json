{
  "name": "command-line-time-tracker",
  "version": "1.2.0",
  "description": "Time Tracker Command Line Tool",
  "main": "commands/tt.js",
  "scripts": {
    "test": "nyc mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:mocha": "mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js",
    "test:watch": "mocha --compilers js:babel-core/register --reporter spec test/**/*.test.js --watch",
    "mocha": "mocha --compilers js:babel-core/register",
    "eslint": "eslint",
    "nyc": "nyc"
  },
  "author": {
    "name": "Jonathan Keller",
    "email": "<keller.jonathan@gmail.com>"
  },
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellerj/command-line-time-tracker.git"
  },
  "dependencies": {
    "chalk": "1.1.x",
    "co": "4.6.x",
    "commander": "git+https://github.com/tj/commander.js.git#3367806abf354cb420477ffe1a9dcaeb7388392f",
    "coveralls": "^2.13.1",
    "debug": "2.6.x",
    "easy-table": "1.1.x",
    "inquirer": "3.0.x",
    "moment": "2.18.x",
    "mongodb": "2.2.x",
    "rx": "4.1.x",
    "sprintf-js": "1.1.x"
  },
  "bin": {
    "tt": "./commands/tt.js",
    "tt-add": "./commands/tt-add.js",
    "tt-edit": "./commands/tt-edit.js",
    "tt-list": "./commands/tt-list.js",
    "tt-delete": "./commands/tt-delete.js",
    "tt-project": "./commands/tt-project.js",
    "tt-project-add": "./commands/tt-project-add.js",
    "tt-project-list": "./commands/tt-project-list.js",
    "tt-project-delete": "./commands/tt-project-delete.js",
    "tt-timetype": "./commands/tt-timetype.js",
    "tt-timetype-list": "./commands/tt-timetype-list.js",
    "tt-timetype-add": "./commands/tt-timetype-add.js",
    "tt-timetype-delete": "./commands/tt-timetype-delete.js",
    "tt-summary": "./commands/tt-summary.js",
    "tt-report": "./commands/tt-report.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "3.19.x",
    "eslint-config-airbnb": "14.1.x",
    "eslint-plugin-import": "2.2.x",
    "eslint-plugin-jsx-a11y": "4.0.x",
    "eslint-plugin-react": "6.10.x",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "sinon": "^2.3.5"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 90,
    "statements": 90,
    "functions": 100,
    "branches": 75,
    "reporter": [
      "lcov",
      "text-summary",
      "text",
      "html"
    ],
    "watermarks": {
      "lines": [
        75,
        90
      ],
      "functions": [
        90,
        100
      ],
      "branches": [
        50,
        75
      ],
      "statements": [
        75,
        90
      ]
    },
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
