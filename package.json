{
  "name": "command-line-time-tracker",
  "version": "2.0.0-0",
  "description": "Time Tracker",
  "main": "./src/commands/tt.js",
  "scripts": {
    "lint": "eslint src --ext .js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "preversion": "npm run lint && npm test",
    "version": "git add src",
    "postversion": "git push && git push --tags",
    "tt": "babel-node src/commands/tt.js",
    "tt-debug": "DEBUG=tt:* babel-node src/commands/tt.js",
    "tt-inspect": "DEBUG=tt:* babel-node --inspect-brk src/commands/tt.js",
    "compile": "babel -d dist/ src/",
    "prepare": "npm run compile",
    "doc": "jsdoc -c ./jsdoc.config.js",
    "watch": "babel --watch -d dist/ src/"
  },
  "author": {
    "name": "Jonathan Keller",
    "email": "keller.jonathan@gmail.com"
  },
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/kellerj/command-line-time-tracker.git"
  },
  "bin": {
    "tt": "./dist/commands/tt.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "d3-scale-chromatic": "^1.3.3",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "easy-table": "^1.1.1",
    "fuzzy": "^0.1.3",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "moment": "^2.24.0",
    "mongodb": "^2.2.35",
    "parse-loose-time": "^1.0.1",
    "sprintf-js": "^1.1.2",
    "stream-buffers": "^3.0.2",
    "supports-color": "^5.4.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-shallow-deep-equal": "^1.4.6",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^3.7.1",
    "istanbul-api": "^1.2.2",
    "istanbul-reports": "^1.1.4",
    "jest": "^23.1.0",
    "jest-runner-eslint": "^0.6.0",
    "jsdoc": "^3.6.3",
    "jsdoc-memberof-namespace": "^2.2.0",
    "minami": "^1.2.3",
    "regenerator-runtime": "^0.11.1",
    "sinon": "^5.0.10"
  },
  "coverageThreshold-temp": {
    "global": {
      "lines": 90,
      "statements": 90,
      "functions": 90,
      "branches": 75
    }
  }
}
